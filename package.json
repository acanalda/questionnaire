{
  "name": "questionnaire",
  "version": "1.0.0",
  "description": "Test Questionnaire project using MobX + React project with JSX, ES6 compilation and hot code reloading",
  "scripts": {
    "prestart": "npm run start-server &",
    "start": "webpack-dev-server",
    "postinstall": "cd src/server && npm install",
    "start-client": "webpack-dev-server",
    "start-server": "./src/server/node_modules/.bin/nodemon src/server/app.js",
    "lint": "eslint src",
    "test": "cd src/server && npm test",
    "reinstall": "rm -rf node_modules && rm -rf src/server/node_modules && npm i"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edmunfollen/questionnaire.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "mobx"
  ],
  "author": "Arturo Canalda <acanalda@gmail.com> (http://github.com/edmunfollen)",
  "license": "MIT",
  "engines": {
    "node" : ">=7.7.1",
    "npm" : ">=4.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1",
    "mocha": "^3.4.2",
    "react-hot-loader": "^3.0.0-beta.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "mobx": "^3.0.0",
    "mobx-react": "^4.1.0",
    "mobx-react-devtools": "^4.2.11",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-router-dom": "^4.1.1"
  }
}
